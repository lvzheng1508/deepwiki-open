# DeepWiki Environment Variables
# Copy this file to .env and fill in your API keys

# Required: At least one API key is required for the application to function

# Google Gemini API Key (Required for Google models)
GOOGLE_API_KEY=your_google_api_key_here

# OpenAI API Key (Required for OpenAI models and embeddings)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Third-party OpenAI-compatible endpoint and default model
OPENAI_BASE_URL=https://your-openai-compatible.example.com/v1
OPENAI_MODEL=your_default_model_name

# Optional: OpenRouter API Key (Required for OpenRouter models)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Azure OpenAI Configuration (Required for Azure models)
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=your_azure_openai_endpoint_here
AZURE_OPENAI_VERSION=your_azure_openai_version_here

# Optional: Ollama Host (default: http://localhost:11434)
OLLAMA_HOST=http://localhost:11434

# Server Configuration
PORT=8001
SERVER_BASE_URL=http://localhost:8001

# Optional: OpenAI-compatible embedding service
OPENAI_EMBED_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
OPENAI_EMBED_KEY=your_embedding_api_key_here

# Optional: Authorization Mode
DEEPWIKI_AUTH_MODE=false
DEEPWIKI_AUTH_CODE=your_secret_auth_code_here

# Optional: Logging Configuration
LOG_LEVEL=INFO
LOG_FILE_PATH=api/logs/application.log

# Optional: Configuration Directory
DEEPWIKI_CONFIG_DIR=/path/to/custom/config/dir
